# Basic Subtractive Synthesis Patch
# Two slightly detuned oscillators through a filter

# Oscillators
vco1: osc saw 220
vco2: osc saw 220.5

# Mix the oscillators
mixer: mix
  <- vco1 * 0.5
  <- vco2 * 0.5

# LFO for filter modulation  
lfo: osc sine 0.3

# Filter with modulated cutoff
vcf: filter moog
  <- mixer
vcf.cutoff <- lfo * 800 + 1200
vcf.res <- 0.7

# Envelope
env: env adsr 10 50 0.7 300

# VCA
vca: vca
  <- vcf
  <- env

# Output with limiter
out <- vca * 0.3